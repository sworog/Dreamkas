<form class="form_refund"
      id="<%- get('id') %>">

    <% model.collections.products.forEach(function(refundProductModel){ %>

    <div name="position"
         data-product-id="<%- refundProductModel.models.product.id %>"
         data-model-cid="<%- refundProductModel.cid %>"
         class="form_refund__position row">

        <div class="col col-md-8">
            <div name="productName" class="form_refund__productName">
                <%- refundProductModel.models.product.get('name') %>
            </div>
            <div name="productBarcode">
                <%- refundProductModel.models.product.get('barcode') %>
            </div>
        </div>

        <div class="col col-md-4">
            <b block="inputNumber"></b>
        </div>

        <div class="form__errorMessage"></div>

    </div>

    <% }) %>

</form>